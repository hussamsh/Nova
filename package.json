{
  "name": "nova",
  "version": "0.0.1",
  "main": "dist/main.js",
  "description": "Desktop application that implements different kinds of cryptographic algorithms",
  "scripts": {
    "build": "webpack --config webpack.config.js",
    "watch": "webpack --config webpack.config.js -w",
    "lint": "tslint -c tslint.json -p tsconfig.json",
    "start": "npm run build && electron ./dist/main.js",
    "test": "mocha ./dist/tests.js",
    "release-win": "electron-packager . Nova --overwrite --asar --platform=win32 protable --arch=ia32 --icon=./app/assets/images/nova.ico --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"Nova Cryptography\" --extraResource=./dist/Encrypt.js --extraResource=./dist/Decrypt.js"
  },
  "build": {
    "appId": "6c2fcd40-3c77-473e-80c1-711ecd86baa2",
    "mac": {
      "category": "cryptography images"
    },
    "directories": {
      "buildResources": "app/assets/images"
    }
  },
  "files": [
    "dist/Encrypt.js",
    "dist/Decrypt.js"
  ],
  "repository": "",
  "keywords": [
    "Electron",
    "quick",
    "start",
    "typescript"
  ],
  "author": "Hossam Sherif",
  "license": "CC0-1.0",
  "devDependencies": {
    "@types/react": "^16.9.34",
    "@types/react-dom": "^16.9.6",
    "css-loader": "^3.5.2",
    "electron": "^8.2.3",
    "electron-packager": "^14.2.1",
    "file-loader": "^6.0.0",
    "html-webpack-plugin": "^4.3.0",
    "mocha-loader": "^5.0.0",
    "node-loader": "^0.6.0",
    "ts-loader": "^6.2.2",
    "tslint": "^5.20.1",
    "typescript": "^3.8.3",
    "webpack": "^4.42.1",
    "webpack-cli": "^3.3.11"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.13.0",
    "@popperjs/core": "^2.3.3",
    "@tippyjs/react": "^4.0.2",
    "@types/chai": "^4.2.11",
    "@types/jquery": "^3.3.35",
    "@types/mathjax": "0.0.36",
    "@types/mathjs": "^6.0.5",
    "@types/mocha": "^7.0.2",
    "@types/styled-components": "^5.1.0",
    "archiver": "^4.0.1",
    "aws-sdk": "^2.696.0",
    "bitset": "^5.1.1",
    "bootstrap": "^4.4.1",
    "chai": "^4.2.0",
    "chalk": "^4.1.0",
    "cli-spinners": "^2.3.0",
    "electron-is-dev": "^1.2.0",
    "file-uri-to-path": "^2.0.0",
    "jimp": "^0.12.1",
    "jquery": "^3.5.0",
    "math-float64-to-binary-string": "^1.0.1",
    "mathjax": "^3.0.5",
    "mathjs": "^7.0.0",
    "minimist": "^1.2.5",
    "mocha": "^7.2.0",
    "node-resemble-js": "^0.2.0",
    "ora": "^4.0.4",
    "popper.js": "^1.16.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "style-loader": "^1.1.4",
    "styled-components": "^5.1.0",
    "typescript-collections": "^1.3.3",
    "uuid": "^7.0.3",
    "webpack-node-externals": "^1.7.2"
  }
}
